{
    "name": "madarlan/aitu-messenger-php",
    "description": "PHP SDK для работы с Aitu Messenger и Aitu Passport API с поддержкой Laravel",
    "type": "library",
    "keywords": [
        "aitu",
        "messenger",
        "passport",
        "oauth",
        "ecp",
        "laravel",
        "php",
        "sdk",
        "api",
        "kazakhstan"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arlan",
            "email": "sovletig@gmail.com"
        }
    ],
    "homepage": "https://github.com/madarlan/aitu-messenger-php",
    "support": {
        "issues": "https://github.com/madarlan/aitu-messenger-php/issues",
        "source": "https://github.com/madarlan/aitu-messenger-php"
    },
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "MadArlan\\AituMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadArlan\\AituMessenger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadArlan\\AituMessenger\\Laravel\\AituMessengerServiceProvider"
            ],
            "aliases": {
                "AituMessenger": "MadArlan\\AituMessenger\\Laravel\\Facades\\AituMessenger",
                "AituPassport": "MadArlan\\AituMessenger\\Laravel\\Facades\\AituPassport"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}